    "benchmark": "^2.1.4",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "microtime": "^3.1.1",
    "onchange": "^7.1.0",
    "prettier": "^2.8.0",
