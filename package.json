  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "tsc -d",
    "dev": "tsc --watch",
    "check": "tsc --no-emit",
    "build": "tsc",
    "build:update": "tsc --build",
    "test": "jest",
    "lint": "eslint . --ext .ts",
